      </trans-unit>
      <trans-unit id="tl_news_archive.delete.0">
        <source>Delete archive</source>
        <target>Slet arkiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.delete.1">
        <source>Delete archive ID %s</source>
        <target>Slet arkiv-ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.0">
        <source>Comments</source>
        <target>Kommentarer</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.1">
        <source>Show comments of archive ID %s</source>
        <target>Vis kommentarer for arkiv-ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feeds.0">
