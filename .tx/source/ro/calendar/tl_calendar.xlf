<?xml version="1.0" ?><xliff version="1.1">
  <file datatype="php" original="system/modules/calendar/languages/en/tl_calendar.php" source-language="en">
    <body>
      <trans-unit id="tl_calendar.title.0">
        <source>Title</source>
        <target>Titlu</target>
      </trans-unit>
      <trans-unit id="tl_calendar.title.1">
        <source>Please enter the calendar title.</source>
        <target>Introduceţi titlul calendarului.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.jumpTo.0">
        <source>Redirect page</source>
        <target>Sari la pagina</target>
      </trans-unit>
      <trans-unit id="tl_calendar.jumpTo.1">
        <source>Please choose the event reader page to which visitors will be redirected when clicking an event.</source>
        <target>Selectaţi pagina către care vor fi redirectaţi vizitatorii atunci când selectează un eveniment.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.allowComments.0">
        <source>Enable comments</source>
        <target>Activează comentarii</target>
      </trans-unit>
      <trans-unit id="tl_calendar.allowComments.1">
        <source>Allow visitors to comment events.</source>
        <target>Permite vizitatorilor să comenteze evenimente.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.notify.0">
        <source>Notify</source>
        <target>Notifică</target>
      </trans-unit>
      <trans-unit id="tl_calendar.notify.1">
        <source>Please choose who to notify when comments are added.</source>
        <target>Alegeţi cine este notificat în momentul adăugării unui comentartiu.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.sortOrder.0">
        <source>Sort order</source>
        <target>Ordine de sortare</target>
      </trans-unit>
      <trans-unit id="tl_calendar.sortOrder.1">
        <source>By default, comments are sorted ascending, starting with the oldest one.</source>
        <target>Îniţial comentariile sunt sortate ascendent începând cu cel mai vechi.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.perPage.0">
        <source>Comments per page</source>
        <target>Comentarii pe pagină</target>
      </trans-unit>
      <trans-unit id="tl_calendar.perPage.1">
        <source>Number of comments per page. Set to 0 to disable pagination.</source>
        <target>Numărul de comentarii pe pagină. Setaţi 0 pentru a dezactiva paginarea.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.moderate.0">
        <source>Moderate comments</source>
        <target>Moderare comentarii</target>
      </trans-unit>
      <trans-unit id="tl_calendar.moderate.1">
        <source>Approve comments before they are published on the website.</source>
        <target>Aprobă comentariile înainte să fie publicate pe website.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.bbcode.0">
        <source>Allow BBCode</source>
        <target>Permite BBCode</target>
      </trans-unit>
      <trans-unit id="tl_calendar.bbcode.1">
        <source>Allow visitors to format their comments with BBCode.</source>
        <target>Permite utilizatorilor să formateze comentariile cu BBCode.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.requireLogin.0">
        <source>Require login to comment</source>
        <target>Autentificare înainte de comentariu</target>
      </trans-unit>
      <trans-unit id="tl_calendar.requireLogin.1">
        <source>Allow only authenticated users to create comments.</source>
        <target>Permite adăugarea de comentarii după autentificare.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.disableCaptcha.0">
        <source>Disable the security question</source>
        <target>Deactivează întrebarea de securitate</target>
      </trans-unit>
      <trans-unit id="tl_calendar.disableCaptcha.1">
        <source>Use this option only if you have limited comments to authenticated users.</source>
        <target>Folosiţi opţiunea aceasta doar dacă este limitată adăugarea de comentarii doar pentru utilizatorii autentificaţi.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.protected.0">
        <source>Protect calendar</source>
        <target>Protejează calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.protected.1">
        <source>Show events to certain member groups only.</source>
        <target>Arată evenimentele doar anumitor gruputi de membri.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.groups.0">
        <source>Allowed member groups</source>
        <target>Permite pentru grupuri</target>
      </trans-unit>
      <trans-unit id="tl_calendar.groups.1">
        <source>These groups will be able to see the events in this calendar.</source>
        <target>Aici puteţi alege căror grupuri de membri li se va permite să vizualizeze evenimentele.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.makeFeed.0">
        <source>Generate feed</source>
        <target>Generează flux RSS</target>
      </trans-unit>
      <trans-unit id="tl_calendar.makeFeed.1">
        <source>Generate an RSS or Atom feed from the calendar.</source>
        <target>Generează un flux RSS/Atom cu evenimente din calendar.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.format.0">
        <source>Feed format</source>
        <target>Formatul fluxului</target>
      </trans-unit>
      <trans-unit id="tl_calendar.format.1">
        <source>Please choose a feed format.</source>
        <target>Alegeti un format de flux.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.language.0">
        <source>Feed language</source>
        <target>Limba</target>
      </trans-unit>
      <trans-unit id="tl_calendar.language.1">
        <source>Please enter the feed language according to the ISO-639 standard (e.g. &lt;em&gt;en&lt;/em&gt; or &lt;em&gt;en-us&lt;/em&gt;).</source>
        <target>Introduceţi limba conform cu ISO-639 (ex.&lt;em&gt;en&lt;/em&gt;, &lt;em&gt;en-us&lt;/em&gt; sau &lt;em&gt;ro&lt;/em&gt;).</target>
      </trans-unit>
      <trans-unit id="tl_calendar.source.0">
        <source>Export settings</source>
        <target>Setări de export</target>
      </trans-unit>
      <trans-unit id="tl_calendar.source.1">
        <source>Here you can choose what will be exported.</source>
        <target>Alegeţi ce va fi exportat.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.maxItems.0">
        <source>Maximum number of items</source>
        <target>Număr maxim de obiecte</target>
      </trans-unit>
      <trans-unit id="tl_calendar.maxItems.1">
        <source>Here you can limit the number of events. Set to 0 to export all.</source>
        <target>Limitaţi numărul obiectelor exportate. Lăsaţi necompletat sau introduceţi 0 pentru a le include pe toate.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.feedBase.0">
        <source>Base URL</source>
        <target>URL de bază</target>
      </trans-unit>
      <trans-unit id="tl_calendar.feedBase.1">
        <source>Please enter the base URL with protocol (e.g. &lt;em&gt;http://&lt;/em&gt;).</source>
        <target>Introduceţi URL-ul de bază incluzând protocolul (ex. &lt;em&gt;http://&lt;/em&gt;).</target>
      </trans-unit>
      <trans-unit id="tl_calendar.alias.0">
        <source>Feed alias</source>
        <target>Alias pentru RSS</target>
      </trans-unit>
      <trans-unit id="tl_calendar.alias.1">
        <source>Here you can enter a unique filename (without extension). The XML feed file will be auto-generated in the root directory of your Contao installation, e.g. as &lt;em&gt;name.xml&lt;/em&gt;.</source>
        <target>Puteţi introduce un nume unic de flux RSS. Un fişier XML va fi generat automat în directorul rădăcină (&lt;em&gt;nume.xml&lt;/em&gt;)</target>
      </trans-unit>
      <trans-unit id="tl_calendar.description.0">
        <source>Feed description</source>
        <target>Descriere</target>
      </trans-unit>
      <trans-unit id="tl_calendar.description.1">
        <source>Please enter a short description of the calendar feed.</source>
        <target>Introduceţi o scurtă descriere a calendarului.</target>
      </trans-unit>
      <trans-unit id="tl_calendar.tstamp.0">
        <source>Revision date</source>
        <target>Data reviziei</target>
      </trans-unit>
      <trans-unit id="tl_calendar.tstamp.1">
        <source>Date and time of the latest revision</source>
        <target>Data şi ora ultimei revizii</target>
      </trans-unit>
      <trans-unit id="tl_calendar.title_legend">
        <source>Title and redirect page</source>
        <target>Titlu şi pagină</target>
      </trans-unit>
      <trans-unit id="tl_calendar.comments_legend">
        <source>Comments</source>
        <target>Cometarii</target>
      </trans-unit>
      <trans-unit id="tl_calendar.protected_legend">
        <source>Access protection</source>
        <target>Protecţie acces</target>
      </trans-unit>
      <trans-unit id="tl_calendar.feed_legend">
        <source>RSS/Atom feed</source>
        <target>Flux RSS</target>
      </trans-unit>
      <trans-unit id="tl_calendar.notify_admin">
        <source>System administrator</source>
        <target>Administrator</target>
      </trans-unit>
      <trans-unit id="tl_calendar.notify_author">
        <source>Author of the event</source>
        <target>Autor al evenimetului</target>
      </trans-unit>
      <trans-unit id="tl_calendar.notify_both">
        <source>Author and system administrator</source>
        <target>Autor şi administrator</target>
      </trans-unit>
      <trans-unit id="tl_calendar.source_teaser">
        <source>Event teasers</source>
        <target>Rezumate evenimente</target>
      </trans-unit>
      <trans-unit id="tl_calendar.source_text">
        <source>Full articles</source>
        <target>Articole complete</target>
      </trans-unit>
      <trans-unit id="tl_calendar.new.0">
        <source>New calendar</source>
        <target>Calendar nou</target>
      </trans-unit>
      <trans-unit id="tl_calendar.new.1">
        <source>Create a new calendar</source>
        <target>Creează calendar nou</target>
      </trans-unit>
      <trans-unit id="tl_calendar.show.0">
        <source>Calendar details</source>
        <target>Detalii calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.show.1">
        <source>Show the details of calendar ID %s</source>
        <target>Arată detaliile calendarului ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.edit.0">
        <source>Edit calendar</source>
        <target>Editează calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.edit.1">
        <source>Edit calendar ID %s</source>
        <target>Editează calendarul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.editheader.0">
        <source>Edit calendar settings</source>
        <target>Editează setări de calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.editheader.1">
        <source>Edit the settings of calendar ID %s</source>
        <target>Editează setări pentru calendarul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.copy.0">
        <source>Duplicate calendar</source>
        <target>Copiază calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.copy.1">
        <source>Duplicate calendar ID %s</source>
        <target>Copiază calendarul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.delete.0">
        <source>Delete calendar</source>
        <target>Şterge calendar</target>
      </trans-unit>
      <trans-unit id="tl_calendar.delete.1">
        <source>Delete calendar ID %s</source>
        <target>Şterge calendarul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.feeds.0">
        <source>RSS feeds</source>
        <target>Fluxuri RSS</target>
      </trans-unit>
      <trans-unit id="tl_calendar.feeds.1">
        <source>Manage RSS feeds</source>
        <target>Gestionare fluxuri RSS</target>
      </trans-unit>
    </body>
