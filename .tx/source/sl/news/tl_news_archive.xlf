  <file datatype="php" original="system/modules/news/languages/en/tl_news_archive.php" source-language="en">
    <body>
      <trans-unit id="tl_news_archive.title.0">
        <source>Title</source>
        <target>Naziv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.title.1">
        <source>Please enter a news archive title.</source>
        <target>Vnesite naziv za arhiv novic.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.jumpTo.0">
        <source>Redirect page</source>
        <target>Pojdi na stran</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.jumpTo.1">
        <source>Please choose the news reader page to which visitors will be redirected when clicking a news item.</source>
        <target>Izberite stran za branje novic na katero bodo obiskovalci preusmerjeni ob kliku na novico.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.allowComments.0">
        <source>Enable comments</source>
        <target>Dovoli komentarje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.allowComments.1">
        <source>Allow visitors to comment news items.</source>
        <target>Dovoli obiskovalcem komentiranje novic.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify.0">
        <source>Notify</source>
        <target>Obveščanje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify.1">
        <source>Please choose who to notify when comments are added.</source>
        <target>Prosimo, izberite koga se obvešča ob novem komentarju.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.sortOrder.0">
        <source>Sort order</source>
        <target>Razvrščanje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.sortOrder.1">
        <source>By default, comments are sorted ascending, starting with the oldest one.</source>
        <target>Privzeto se komentarji razvrščajo naraščajoče, z najstarejšim naprej.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.perPage.0">
        <source>Comments per page</source>
        <target>Komentarjev na stran</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.perPage.1">
        <source>Number of comments per page. Set to 0 to disable pagination.</source>
        <target>Število komentarjev na stran. Nastavite na 0 če ne želite številčenja (paginacije).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.moderate.0">
        <source>Moderate comments</source>
        <target>Moderiraj komentarje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.moderate.1">
        <source>Approve comments before they are published on the website.</source>
        <target>Odobrite komentarje preden so prikazani na strani.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.bbcode.0">
        <source>Allow BBCode</source>
        <target>Dovoli BBCode</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.bbcode.1">
        <source>Allow visitors to format their comments with BBCode.</source>
        <target>Dovoli obiskovalcem uporabo BB kode za oblikovanje njihovih komentarjev.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.requireLogin.0">
        <source>Require login to comment</source>
        <target>Zahtevana prijava za komentiranje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.requireLogin.1">
        <source>Allow only authenticated users to create comments.</source>
        <target>Komentiranje dovoli samo prijavljenim uporabnikom.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.disableCaptcha.0">
        <source>Disable the security question</source>
        <target>Onemogoči varnostno vprašanje</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.disableCaptcha.1">
        <source>Use this option only if you have limited comments to authenticated users.</source>
        <target>Izberite to možnost, če želite onemogočiti varnostno vprašanje (ni priporočljivo).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected.0">
        <source>Protect archive</source>
        <target>Zaščiti arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected.1">
        <source>Show news items to certain member groups only.</source>
        <target>Prikaži novice samo določeni skupini članov.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.groups.0">
        <source>Allowed member groups</source>
        <target>Dovoljene skupine članov</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.groups.1">
        <source>These groups will be able to see the news items in this archive.</source>
        <target>Tem skupinam bo omogočeno branje novic v tem arhivu.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.makeFeed.0">
        <source>Generate feed</source>
        <target>Ustvari RSS/Atom izvleček</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.makeFeed.1">
        <source>Generate an RSS or Atom feed from the news archive.</source>
        <target>Generiraj RSS ali Atom izvleček iz arhiva novic.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.format.0">
        <source>Feed format</source>
        <target>Oblika izvlečka</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.format.1">
        <source>Please choose a feed format.</source>
        <target>Prosimo izberite obliko izvlečka.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.language.0">
        <source>Feed language</source>
        <target>Jezik izvlečka</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.language.1">
        <source>Please enter the feed language according to the ISO-639 standard (e.g. &lt;em&gt;en&lt;/em&gt; or &lt;em&gt;en-us&lt;/em&gt;).</source>
        <target>Prosimo vpišite jezik glede na ISO-639 standard (npr.: &lt;em&gt;en&lt;/em&gt; ali &lt;em&gt;en-us&lt;/em&gt;).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source.0">
        <source>Export settings</source>
        <target>Nastavitve izvoza</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source.1">
        <source>Here you can choose what will be exported.</source>
        <target>Tukaj lahko izberete kaj bo izvoženo.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.maxItems.0">
        <source>Maximum number of items</source>
        <target>Največje število elementov izvlečka</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.maxItems.1">
        <source>Here you can limit the number of feed items. Set to 0 to export all.</source>
        <target>Tukaj lahko omejite število elementov izvlečka. Nastavite na 0 za izvoz vseh.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feedBase.0">
        <source>Base URL</source>
        <target>Izhodiščni URL</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feedBase.1">
        <source>Please enter the base URL with protocol (e.g. &lt;em&gt;http://&lt;/em&gt;).</source>
        <target>Prosimo vpišite URL s protkolom (npr.: &lt;em&gt;http://&lt;/em&gt;).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.alias.0">
        <source>Feed alias</source>
        <target>Vzdevek izvlečka (alias)</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.alias.1">
        <source>Here you can enter a unique filename (without extension). The XML feed file will be auto-generated in the root directory of your Contao installation, e.g. as &lt;em&gt;name.xml&lt;/em&gt;.</source>
        <target>Tukaj lahko vpišete edinstveno ime datoteke (brez razširitvene končnice ). Datoteka XML izvlečka bo generirana avtomatsko v &quot;root&quot; mapi vaše TYPOlight inštalacije, kot npr.: &lt;em&gt;name.xml&lt;/em&gt;.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.description.0">
        <source>Feed description</source>
        <target>Opis izvlečka</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.description.1">
        <source>Please enter a short description of the news feed.</source>
        <target>Prosimo vnesite kratek opis izvlečka novic.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.tstamp.0">
        <source>Revision date</source>
        <target>Datum revizije</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.tstamp.1">
        <source>Date and time of the latest revision</source>
        <target>Datum in čas zadnje revizije.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.title_legend">
        <source>Title and redirect page</source>
        <target>Naslov in preusmeritvena stran</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments_legend">
        <source>Comments</source>
        <target>Komentarji</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected_legend">
        <source>Access protection</source>
        <target>Omejitev dostopa</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feed_legend">
        <source>RSS/Atom feed</source>
        <target>RSS/Atom izvleček</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_admin">
        <source>System administrator</source>
        <target>Administrator sistema</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_author">
        <source>Author of the news item</source>
        <target>Avtor novice</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_both">
        <source>Author and system administrator</source>
        <target>Avtor in administrator sistema</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source_teaser">
        <source>News teasers</source>
        <target>Uvodnik novice</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source_text">
        <source>Full articles</source>
        <target>Celotne novice</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.new.0">
        <source>New archive</source>
        <target>Nov arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.new.1">
        <source>Create a new archive</source>
        <target>Ustvari nov arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.show.0">
        <source>Archive details</source>
        <target>Podrobnosti arhiva</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.show.1">
        <source>Show the details of archive ID %s</source>
        <target>Prikaži podrobnosti arhiva ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.edit.0">
        <source>Edit archive</source>
        <target>Uredi arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.edit.1">
        <source>Edit archive ID %s</source>
        <target>Uredi arhiv ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.editheader.0">
        <source>Edit archive settings</source>
        <target>Uredi nastavitve arhiva</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.editheader.1">
        <source>Edit the settings of archive ID %s</source>
        <target>Uredi nastavitve arhiva ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.copy.0">
        <source>Duplicate archive</source>
        <target>Kopiraj arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.copy.1">
        <source>Duplicate archive ID %s</source>
        <target>Kopiraj arhiv ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.delete.0">
        <source>Delete archive</source>
        <target>Izbriši arhiv</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.delete.1">
        <source>Delete archive ID %s</source>
        <target>Izbriši arhiv ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.0">
        <source>Comments</source>
        <target>Komentarji</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.1">
        <source>Show comments of archive ID %s</source>
        <target>Prikaži komentarje arhiva ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feeds.0">
        <source>RSS feeds</source>
        <target>RSS izvlečki</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feeds.1">
        <source>Manage RSS feeds</source>
        <target>Upravljaj RSS izvlečke</target>
      </trans-unit>
    </body>
